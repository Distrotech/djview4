@SET_MAKE@
SHELL = @SHELL@
INSTALL = @INSTALL@
LN_S = @LN_S@
RM = @RM@ 
LIBTOOL = @LIBTOOL@
LIBTOOL_DEPS = @LIBTOOL_DEPS@

srcdir = @srcdir@
prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
datadir = @datadir@
libdir = @libdir@
mandir = @mandir@

srcdir = @srcdir@
top_srcdir = @top_srcdir@
top_builddir = .

SUBDIRS=src


all: FORCE
	for d in ${SUBDIRS} ; do make -C $$d all ; true ; done
	@echo "Done."


clean-here: FORCE

distclean-here: clean-here FORCE
	-rm 2>/dev/null Makefile config.log config.status config.h libtool
	-rm 2>/dev/null -r autom4te.cache

clean: clean-here
	for d in ${SUBDIRS} ; do make -C $$d clean ; true ; done

distclean: distclean-here 
	for d in ${SUBDIRS} ; do make -C $$d distclean ; true ; done

FORCE:

.PHONY: FORCE





