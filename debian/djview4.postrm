#! /bin/sh

# Alternatives
if [ "$1" = "remove" ] && [ -x /usr/sbin/update-alternatives ] ; then
  /usr/sbin/update-alternatives --quiet --remove djview /usr/bin/djview4
fi

# Register menu entries (non debian menus)
if [ "$1" = "remove" ] && 
   [ -x /usr/bin/update-desktop-database ] ; then
  /usr/bin/update-desktop-database
fi

