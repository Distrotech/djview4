#! /bin/sh

case "$1" in
    configure)
	# Alternatives
	if [ -x /usr/sbin/update-alternatives ] ; then
	    update-alternatives --quiet \
		--install /usr/bin/djview djview /usr/bin/djview4 40 \
		--slave \
		/usr/share/man/man1/djview.1.gz djview.1.gz \
		/usr/share/man/man1/djview4.1.gz
	fi
	;;
esac

